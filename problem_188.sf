# Find the last 8 digits of 1777↑↑1855.

# https://projecteuler.net/problem=188

func hyper(n, (1)) { n }

func hyper(n, k) {
    expmod(n, hyper(n, k-1), 1e8)
}

say hyper(1777, 1855)
