# Runtime: 5.110s (under -O1)

var list = (1..100_000 -> map { |i|
    [i.is_square_free ? i : i.rad, i]
})

list.sort! {|a,b|
           (a[0] <=> b[0]) ||
           (a[1] <=> b[1])
}

say list[10000-1][1]
